include ../mixins/mixin-menu.jade

block vars
	- var bodyClass = "page-" + title.toLowerCase()
	- var headerImage = ""
	- var title = title || ''

extends layout-html5

append head

append body

	.sister-sledge
		header.l-header
			//-.inner
			block header

		section.l-main(role="main")
			//-.inner
			block main

		footer.l-footer
			//-.inner
			block footer
				include layout-footer

	#starwarswrap
		canvas#starwars

	block scripts
		script(src="scripts/plugins.min.js")
		script(src="scripts/main.min.js")
		script.
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

			ga('create', 'UA-3906535-20', 'oskarrough.com');
			ga('send', 'pageview');
